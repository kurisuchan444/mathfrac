{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Minimal boilerplate with React 16, Babel 8, Webpack 4, hot loading",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "dev": "webpack --mode development",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development --open",
    "compile": "webpack --mode production",
    "build": "webpack --mode production",
    "clean": "rm dist/*",
    "distclean": "rm dist/*; rm -rf node_modules; rm package-lock.json",
    "web": "pushd dist; python -m http.server; popd",
    "php": "pushd dist; php -S 192.168.1.126:8000; popd",
    "php1": "pushd dist; php -S localhost:8000; popd",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Christian Mannes <christ@mannes-tech.com> (financegear.com)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-import": "^1.9.1",
    "babel-preset-mobx": "^2.0.0",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.2",
    "react-hot-loader": "^4.3.8",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.8"
  },
  "dependencies": {
    "@material-ui/core": "^3.1.2",
    "@material-ui/icons": "^3.0.1",
    "less": "^3.8.1",
    "mobx": "^5.1.1",
    "mobx-react": "^5.2.8",
    "mobx-react-devtools": "^6.0.3",
    "mobx-router": "0.0.13",
    "moment": "^2.22.2",
    "react": "^16.5.1",
    "react-data-grid": "^4.0.8",
    "react-dom": "^16.5.1",
    "react-sketchpad": "0.0.3"
  }
}
